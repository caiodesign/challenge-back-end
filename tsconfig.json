{
  "compilerOptions": {
  "baseUrl": "src",
  "target": "es6",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "resolveJsonModule": true,
    "lib": ["es6", "dom"],
    "esModuleInterop": true,
    "strict": true,
    "inlineSourceMap": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
